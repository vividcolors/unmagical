{
  "name": "unmagical", 
  "version": "0.0.1", 
  "description": "frontend framework for typical applications", 
  "keywords": ["jsx", "typescript", "tsx", "frontend", "framework"], 
  "license": "MIT", 
  "dependencies": {
    "ajv": "^8.1.0",
    "hyperapp": "^1.2.10",
    "rambda": "^6.7.0",
    "ramda": "^0.27.1",
    "string-template": "^1.0.0",
    "typescript": "4.4"
  },
  "scripts": {
    "buildbulma": "parcel build --global unmagical -d asset src/unmagical-bulma.ts", 
    "buildnode": "parcel build -d . src/index.ts --target node", 
    "build": "run-s buildbulma buildnode",
    "test": "parcel build --target node -d asset/test tests/_index.js && node asset/test/_index",
    "demo2": "parcel build -d demo/demo2 --out-file index.js demo/demo2/src.tsx",
    "demo4": "parcel build -d demo/demo4 --out-file index.js demo/demo4/src.js",
    "demo5": "parcel build -d demo/demo5 --out-file index.js demo/demo5/src.tsx",
    "docs": "typedoc --options typedoc.json ./src"
  },
  "devDependencies": {
    "@types/ramda": "^0.27.50",
    "babel-core": "^6.26.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cpx": "^1.5.0",
    "json-server": "^0.17.0",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "rimraf": "^3.0.0",
    "sass": "^1.22.1",
    "typedoc": "^0.22.9"
  }
}
